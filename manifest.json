{
    "manifest_version": 3,
    "name": "Extension Example",
    "version" : "0.0.1",
    "description": "Extension Example to learn extension development basics",
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        },
        "default_title": "Extension Example"
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*.google.com/*"
            ],
            "css": [
                "contentStyle.css"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ]
}